{
  "name": "Hashword",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Generate a unique, secure password for every web site from one master password.",
  "background": {
    "page": "pages/background/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Hashword",
    "default_popup": "pages/popup/popup.html",
    "default_icon": {
      "16": "images/icon16-inactive.png",
      "19": "images/icon19-inactive.png",
      "32": "images/icon32-inactive.png",
      "38": "images/icon38-inactive.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      }
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "incognito": "spanning",
  "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQC3ZajsSS+Z19eOXpwBhjvaQpnTtMz00+tWlidTKnopWmbpynsrTGJD8aDaoMufcPKhOPmsjL04EKX+L31WnDTOwvDZ/qcejS1cWnlOnLjwZqOMrhM0KraF+nD2PmWUFWOBi4O0r6oq3aJjrrQEoa7ZPZ8qnCkAAd7zB5ZgTWfadQIDAQAB",
  "minimum_chrome_version": "46",
  "options_ui": {
    "page": "pages/options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "activeTab",
    "alarms",
    "clipboardWrite",
    "declarativeContent",
    "storage"
  ]
}
