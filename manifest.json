{
    "name": "Hashword",
    "version": "0.1.0.0",
    "manifest_version": 2,
    "description": "Generate reproducible hashed passwords",
    "background": {
        "scripts": ["lib/sha3.js", "lib/enc-base64-min.js", "lib/tld.js", "background.js"],
        "persistent": false
    },
    "commands": {
        "insert_password": {
            "suggested_key": {
                "default": "Ctrl+Shift+P",
                "mac": "Command+Shift+P"
            },
            "description": "Insert Password"
        }
    },
    "incognito": "spanning",
    "minimum_chrome_version": "26",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ]
}
